#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/prabath/Documents/Dep-10/phase-4/angular-basic/library-app/frontend/angular-frontend-app/node_modules/.pnpm/needle@3.2.0/node_modules/needle/bin/node_modules:/home/prabath/Documents/Dep-10/phase-4/angular-basic/library-app/frontend/angular-frontend-app/node_modules/.pnpm/needle@3.2.0/node_modules/needle/node_modules:/home/prabath/Documents/Dep-10/phase-4/angular-basic/library-app/frontend/angular-frontend-app/node_modules/.pnpm/needle@3.2.0/node_modules:/home/prabath/Documents/Dep-10/phase-4/angular-basic/library-app/frontend/angular-frontend-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/prabath/Documents/Dep-10/phase-4/angular-basic/library-app/frontend/angular-frontend-app/node_modules/.pnpm/needle@3.2.0/node_modules/needle/bin/node_modules:/home/prabath/Documents/Dep-10/phase-4/angular-basic/library-app/frontend/angular-frontend-app/node_modules/.pnpm/needle@3.2.0/node_modules/needle/node_modules:/home/prabath/Documents/Dep-10/phase-4/angular-basic/library-app/frontend/angular-frontend-app/node_modules/.pnpm/needle@3.2.0/node_modules:/home/prabath/Documents/Dep-10/phase-4/angular-basic/library-app/frontend/angular-frontend-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
